{
  "name": "aura-front",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:env": "node scripts/load-env.js expo start",
    "start:dev": "cross-env API_URL=http://localhost:3000/api NODE_ENV=development expo start -c",
    "start:prod": "cross-env API_URL=https://back.aurapp.com.mx/api NODE_ENV=production APP_ENV=production expo start -c",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "android:env": "node scripts/load-env.js expo start --android",
    "ios": "expo start --ios",
    "ios:env": "node scripts/load-env.js expo start --ios",
    "web": "expo start --web",
    "web:env": "node scripts/load-env.js expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "env:dev": "node ./scripts/switch-env-simple.js dev && expo start -c",
    "env:prod": "node ./scripts/switch-env-simple.js prod && expo start -c"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "overrides": {
    "react-dom": "19.1.0"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@shopify/react-native-skia": "2.2.12",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^7.9.0",
    "@stripe/stripe-react-native": "0.50.3",
    "expo": "^54.0.9",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.12",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.14",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-head": "^0.0.20",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-media-library": "~18.2.0",
    "expo-router": "~6.0.7",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "jwt-decode": "^4.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-share": "^12.1.0",
    "react-native-svg": "15.12.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^18.3.0",
    "cross-env": "^10.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.12",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.9.2"
