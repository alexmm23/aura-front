{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/(auth)/_layout.jsx",
    "app/_layout.jsx",
    "app/(auth)/login.jsx",
    "app/index.tsx",
    "constants/Colors.js",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
