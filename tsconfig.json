{
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/(auth)/_layout.jsx",
    "app/_layout.jsx",
    "app/(auth)/login.jsx",
    "app/index.jsx",
    "constants/Colors.js",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "extends": "expo/tsconfig.base"
}
