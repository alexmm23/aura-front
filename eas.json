{
  "build": {
    "development": {
      "android": {
        "buildType": "apk",
        "developmentClient": true,
        "env": {
          "APP_ENV": "development"
        },
        "distribution": "internal"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "env": {
          "APP_ENV": "staging"
        }
      }
    },
    "production": {
      "android": {
        "env": {
          "APP_ENV": "production"
        }
      }
    }
  }
}
